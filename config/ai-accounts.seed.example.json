{
  "_comment": "AI 账号种子数据示例文件",
  "_usage": "加载方式: oxmon-server init-ai-accounts <config.toml> <this-file.json>",
  "_note": "AI 账号存储在 system_configs 表中，config_type='ai_account'",

  "ai_accounts": [
    {
      "config_key": "ai_zhipu_default",
      "provider": "zhipu",
      "display_name": "智谱 GLM-5 默认账号",
      "description": "用于生成每日巡检报告的默认 AI 账号",
      "enabled": true,
      "config": {
        "api_key": "your_zhipu_api_key_here",
        "model": "glm-5",
        "base_url": "https://open.bigmodel.cn/api/paas/v4",
        "timeout_secs": 120,
        "max_tokens": 4096,
        "temperature": 0.7,
        "collection_interval_secs": 86400
      }
    },
    {
      "config_key": "ai_kimi_backup",
      "provider": "kimi",
      "display_name": "Kimi 备用账号",
      "description": "备用 AI 模型（暂未实现）",
      "enabled": false,
      "config": {
        "api_key": "your_kimi_api_key_here",
        "model": "moonshot-v1-32k",
        "base_url": "https://api.moonshot.cn/v1",
        "timeout_secs": 180,
        "max_tokens": 8192,
        "temperature": 0.7,
        "collection_interval_secs": 86400
      }
    },
    {
      "config_key": "ai_custom_local",
      "provider": "custom",
      "display_name": "自定义本地模型",
      "description": "使用 OpenAI 兼容接口的本地模型",
      "enabled": false,
      "config": {
        "api_key": "sk-xxx",
        "model": "custom-model-v1",
        "base_url": "http://localhost:8080/v1",
        "timeout_secs": 300,
        "max_tokens": 4096,
        "temperature": 0.7,
        "request_format": "openai_compatible",
        "collection_interval_secs": 86400,
        "headers": {
          "X-Custom-Header": "value"
        }
      }
    }
  ]
}
